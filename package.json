{
  "name": "omni-python-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:wasm": "wasm-pack build ruff-source/crates/ty_wasm --target web --out-dir ../../../ty_wasm",
    "predev": "yarn build:wasm",
    "dev": "vite",
    "prebuild": "yarn build:wasm",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "yarn build:wasm"
  },
  "dependencies": {
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/state": "^6.5.3",
    "@codemirror/view": "^6.39.7",
    "@uiw/react-codemirror": "^4.23.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ty_wasm": "file:ty_wasm"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  },
  "volta": {
    "node": "25.2.1",
    "yarn": "4.12.0"
  }
}
